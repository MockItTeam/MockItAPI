<div class='editor-container'>

  <div class='editor-menu-container'>
    <nav class='editor-menu navbar'>
      <div class='editor-menu-tools'>
        <button class='btn btn-icon'><i class='fa fa-undo'></i></button>
        <button class='btn btn-icon'><i class='fa fa-repeat'></i></button>
        <button class='btn btn-icon'><i class='fa fa-copy'></i></button>
        <button class='btn btn-icon'><i class='fa fa-paste'></i></button>
        <button class='btn btn-icon'><i class='fa fa-trash'></i></button>
        <button {{action 'exportToImage'}} class='btn btn-icon'><i class='fa fa-download'></i></button>
        <button {{action 'toggleRawImage'}} class='btn btn-icon'><i class='fa fa-file-image-o'></i></button>
      </div>

      <div class='editor-mockup-name'>
        Mockup {{mockup.name}}
      </div>
    </nav>
